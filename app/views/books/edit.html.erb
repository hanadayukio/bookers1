<h1>Editting Book</h1>

<%= form_with model: @lists do |f| %>
    <div id="error_explanation">
        <h2><%= flash[:alrat] %></h2>
    </div>
    <div class="field">
        <label for="book_title">Title</label>
        <%= f.text_field :title %>

    </div>
    <div class="field">
        <label for="book_body">Body</label>
        <%= f.text_area :body %>

    </div>
    <div class="actions">
        <%= f.submit'Update Book' %>
    </div>
    <%= link_to"Edit", "/books/:id/edit" %> | <%= link_to"Back", "/books" %>
    
<% end %>
